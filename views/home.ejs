<%- include('partials/header') %>
    <div class="container-fluid">
      <h1>SHARMA JII SHADDI WALE</h1>
      <h3>Get Your Perfect Match</h3>
      <form action="/" class="Search-form" method="post">
        <div class="row">

          <div class="col-lg-3 col-md-6 col-sm-12">
            <label>Gender</label><br />
            <select class="form-select" name="Gender" id="Gender">
              <option value="Male">Male</option>
              <option value="Female">Female</option>
            </select>
          </div>
          <div class="col-lg-3 col-md-6 col-sm-12">
            <label>Age Between</label><br />
            <select class="form-select" name="Age" id="Age">
              <option value="Any">Any</option>
                <option value="18-22">18-22</option>
                <option value="22-25">22-25</option>
                <option value="25-30">25-30</option>
                <option value="30-35">30-35</option>
                <option value="35+">35+</option>
            </select>
          </div>
          <div class="col-lg-3 col-md-6 col-sm-12">
            <label>Occupation</label><br />
            <select class="form-select" name="Occupation" id="Occupation">
              <option value="Any">Any</option>
                <option value="Engineer">Engineer</option>
                <option value="Doctor">Doctor</option>
                <option value="Business">Business</option>
                <option value="Farming">Farming</option>
                <option value="Goverment-job">Goverment-job</option>
                <option value="Lawyer">Lawyer</option>
                <option value="Other">Other</option>
            </select>
          </div>
          <div class="col-lg-3 col-md-6 col-sm-12">
            <label>City</label><br />
            <select class="form-select" name="City" id="City">
              <option value="Any">Any</option>
                <option value="Indore">Indore</option>
                <option value="Neemuch">Neemuch</option>
                <option value="Dewas">Dewas</option>
            </select>
          </div>

        </div>
        <input class="btn btn-danger btn-lg" type="submit" name="" value="Search">
      </form>

    </div>

